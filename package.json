    "url": "https://github.com/jaysalvat/super-storage/issues"
  },
  "homepage": "https://github.com/jaysalvat/super-storage#readme",
  "devDependencies": {
    "@jaysalvat/eslint-config": "^1.0.2",
    "chai": "^4.3.0",
    "eslint": "^7.20.0",
    "light-server": "^2.9.1",
    "mocha": "^8.3.0",
    "rollup": "^2.39.0",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-terser": "^7.0.2",
    "shelljs": "^0.8.4"
